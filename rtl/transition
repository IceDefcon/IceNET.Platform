struct pos {
    int x[3];
    int y[3];
};

pos positions[2];

int init(void)
{
	positions[0].x[0] = 1;
	positions[0].x[1] = 2;
	positions[0].x[2] = 3;

	positions[1].x[0] = 4;
	positions[1].x[1] = 5;
	positions[1].x[2] = 6;

}

int function(struct pos *P)
{
    P->x[0] = 1;
    // You may want to return an integer from the function
    return 0;
}

int main(int argc, char const *argv[])
{
	init();

	/* code */
	return 0;
}